	public static void main(String[] args) {
		// 성적을 입력받아서 학점을 출력
		Scanner scan = new Scanner(System.in);
		int score = 0;	// 지역변수는 초기화가 필수
		String grade = "F";
		
		if(score < 0 || score >100) {
			System.out.println("범위를 초과했습니다.");
			System.exit(0);
		}
	}
}
