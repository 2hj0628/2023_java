package org;

public class work03 {
	
	public static void main(String[] args) {
		
		 for(int dan=2; dan<=9; dan+=3) {
			 for(int cnt=0; cnt<3 && dan+cnt<10; cnt++) {
				 System.out.printf("***%d´Ü***\t",dan+cnt);
			 }
			 System.out.println();
			 for(int i=1; i<=9; i++) {
				 for(int cnt=0; cnt<3 && dan+cnt<10; cnt++) {
					 System.out.printf("%d X %d = %d\t",dan+cnt,i,(dan+cnt)*i);
				 }
				 System.out.println();
			 }
		 }
	}
}
